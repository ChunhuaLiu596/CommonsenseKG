{
	"training_data": "../../datasets/",
	"output": "../../output/results/",
	"dataset_division": "721_5fold",

    "embedding_module": "IPTransE",
	"search_module": "greedy",

    "dim": 100,
    "rel_hidden_dim": 200,
    "init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss_norm": "L2",

	"learning_rate": 0.01,
    "optimizer": "Adagrad",
	"max_epoch":1500,
	"batch_size": 5000,

	"margin": 1.5,
	"path_parm": 0.1,

    "neg_sampling": "uniform",
	"neg_triple_num": 1,

	"batch_threads_num": 2,
	"test_threads_num": 4,

	"ordered": true,
	"start_valid": 500,
	"max_epoch_rel": 500,
	"eval_freq": 10,
	"train_rel_freq":20,
	"stop_metric": "hits1", 
	"eval_metric": "inner",
	"early_stop_patience":3,
	"csls":10,
	"top_k": [1, 5, 10, 50],
	"is_save": false,
	"eval_norm": true,
	"sim_th": 0.7,
	"bp_freq": 100,
	"beta1":1,
	"beta2":1,
	"rel_inp_dropout":0.2,
	"rel_hid_dropout":0.1,
	"predict_relation":true,
	"train_kg": "kg2",
	"alignment_module": "sharing",
	"load_pretrain_emb": false,
	"mode":"train"
}
